{
    "manifest_version": 3,
    "name": "Upwork Total Earnings",
    "description": "Display Total earnings on Upwork profile page regardless of increment step.",
    "version": "1.0",
    "icons": {
        "16": "assets/icons/icon128.png",
        "48": "assets/icons/icon128.png",
        "128": "assets/icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.upwork.com/freelancers/*"],
            "js": ["content.js"]
        }
    ]
}